<div class="container text-center">
  
    <h1>Welcome to your Address Book</h1><br>
    <% if user_signed_in? %>
      <%= link_to "View Contacts", contacts_path, class: 'btn btn-primary' %>
    <% else %> 
      <%= link_to "Login", new_user_session_path, class: 'btn btn-primary' %><br><br>
      <%= link_to "Register", new_user_registration_path, class: 'btn btn-primary' %>
    <% end %><br><br>
  
</div>
